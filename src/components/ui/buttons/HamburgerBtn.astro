<div class="ml-auto">
  <button
    type="button"
    class="hs-collapse-toggle flex h-12 w-12 items-center justify-center rounded-full text-sm font-bold text-neutral-600 hover:bg-neutral-200 disabled:pointer-events-none disabled:opacity-50 lg:hidden"
    data-hs-collapse="#navbar-collapse-with-animation"
    aria-controls="navbar-collapse-with-animation"
    aria-label="Toggle navigation"
  >
    <svg
      class="h-[3rem] w-[3rem] flex-shrink-0 hs-collapse-open:hidden"
      width="24"
      height="24"
      viewBox="0 0 24 24"
      fill="none"
      stroke="currentColor"
      stroke-width="2"
      stroke-linecap="round"
      stroke-linejoin="round"
    >
      <line x1="3" x2="21" y1="6" y2="6"></line>
      <line x1="3" x2="21" y1="12" y2="12"></line>
      <line x1="3" x2="21" y1="18" y2="18"></line>
    </svg>
    <svg
      class="hidden h-[3em] w-[3rem] flex-shrink-0 hs-collapse-open:block"
      width="24"
      height="24"
      viewBox="0 0 24 24"
      fill="none"
      stroke="currentColor"
      stroke-width="2"
      stroke-linecap="round"
      stroke-linejoin="round"
    >
      <path d="M18 6 6 18"></path>
      <path d="m6 6 12 12"></path>
    </svg>
  </button>
</div>
